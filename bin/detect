#!/usr/bin/env python
import  sys, os
build_dir= sys.argv[1]

print "jexus-build-pack.detect: build dir %s" % build_dir
webConfigFile = os.path.join(build_dir, 'Web.config');

print "jexus-build-pack.detect: webConfigFile %s" % webConfigFile
if os.path.exists(webConfigFile) :
    print '%s exists, use jexus-build-pack.'% webConfigFile;
    exit(0);
else:
    print '%s not exists, can not use jexus-build-pack.' % webConfigFile;
    exit(1);


